#include <stdio.h>

int main()
{
    int a;
    printf("Input a number between 0 and 9: ");
    scanf("%d", &a);
    
    if(a < 5) {
        printf("Your number is less than 5, folk!");
    }
    else if(a > 5) {
        printf("Your number is greater than 5, folk!");
    }
    else
        printf("Your number is 5, folk!");

    return 0;
}
